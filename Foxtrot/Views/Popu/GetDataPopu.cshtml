@model IEnumerable<Foxtrot.Models.DataValuesPopu>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Species)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PopValue)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Species)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PopValue)
        </td>
        
    </tr>
}

</table>
